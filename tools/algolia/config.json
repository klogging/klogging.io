{
  "index_name": "prod",
  "start_urls": ["https://klogging.io/"],
  "sitemap_urls": ["https://klogging.io/sitemap.xml"],
  "stop_urls": [
    "https://klogging.io/blog/tags.*",
    "https://klogging.io/cdn-cgi/l/email-protection",
    "https://klogging.io/search.*"
  ],
  "strip_chars": "#",
  "selectors": {
    "lvl0": {
      "selector": "(//nav[contains(@class, 'navbar')]//div[contains(@class, 'navbar__items')]//a[contains(@class, 'navbar__link--active')]/text())",
      "type": "xpath",
      "global": true,
      "default_value": "Klogging"
    },
    "lvl1": "article h1",
    "lvl2": "article h2",
    "lvl3": "article h3",
    "lvl4": "article h4",
    "lvl5": "article h5",
    "text": "article p, article li"
  },
  "custom_settings": {
    "attributesForFaceting": ["language", "version", "type", "docusaurus_tag"]
  }
}
